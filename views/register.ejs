  <body>

    <div class="container">

      <form class="form-signin" action="/ajax/register" method="post">
        <h2 class="form-signin-heading">회원가입 하세요</h2>
        <label for="name" class="sr-only">이름</label>
        <input type="text" id="name" name="name" class="form-control" placeholder="이름" required autofocus>
        <label for="birth" class="sr-only">생년월일</label>
        <input type="date" id="birth" name="birth" class="form-control" placeholder="생년월일" required>
		<label for="accessToken" class="sr-only">보안토큰</label>
		<input type="password" id="accessToken" name="accessToken" class="form-control" placeholder="보안토큰" required>
        <button class="btn btn-lg btn-primary btn-block" type="submit">회원가입</button><br />
      </form>
		<a href="/login" class="no-deco">
			<button class="btn btn-lg btn-primary btn-block btn-outof-form" type="submit">로그인 페이지로 돌아가기</button>
		</a>
		

    </div> <!-- /container -->


    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
